{
	"compilerOptions": {
		"baseUrl": ".",
		"target": "ES5",

		"noEmit": true,
		"resolveJsonModule": true,
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": false,
		"incremental": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"isolatedModules": true,
		"jsx": "preserve"
	},
	"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "ava.config.js"],
	"exclude": ["node_modules"]
}

